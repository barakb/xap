	<operation name="getClusterPolicy" type="com.j_spaces.core.cluster.ClusterPolicy" impact="ACTION" description="Returns Cluster Policy instance."/>

	<operation name="getRuntimeInfo" type="com.j_spaces.core.admin.SpaceRuntimeInfo" impact="ACTION" description="Returns RuntimeInfo instance."/>
	
	<operation name="getTemplatesInfo" type="java.util.List" impact="ACTION" description="Returns List of templates info.">
		<signature>
			<parameter name="className" type="java.lang.String" description="Class name."/>
		</signature>
	</operation>

	<operation name="getURL" type="com.j_spaces.core.client.SpaceURL" impact="ACTION" description="SpaceURL instance which was used to initialize the space."/>

