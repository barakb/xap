	<!-- Operation: * - Required, other is optional
		*name - The name of the method.
		*type - The type of the method's return value.
		*impact - The impact of the method, one of INFO, ACTION, ACTION_INFO, UNKNOWN.
		description - A human readable description of the operation.
		signature - MBeanParameterInfo objects describing the parameters(arguments) of the method.

		Signature: the list of parameters info
		Parameter: * - Required, other is optional
		*name - The name of the data
		*type - The type or class name of the data
		description - A human readable description of the data.
	-->
	<operation name="getSpace" type="com.j_spaces.core.IJSpace" impact="ACTION" description="Returns the space proxy that this manageable bean governs."/>
	<operation name="getStatistics" type="com.j_spaces.core.filters.StatisticsContext" impact="ACTION" description="Returns a StatisticsContext for specific filter operation code">
		<signature>
			<parameter name="operationCode" type="java.lang.Integer" description="Filter operation code"/>
		</signature>
	</operation>
	<!-- operation name="clean" type="void" impact="ACTION" description="Cleans this space."/-->
	<operation name="count" type="int" impact="ACTION" description="Returns the number of entries.">
		<signature>
			<parameter name="className" type="java.lang.String" description=""/>
		</signature>
	</operation>
